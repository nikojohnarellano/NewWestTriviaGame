<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_highscore"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="a00953080.comp3717.bcit.ca.newwesttriviaapp.activities.HighscoreActivity">

    <TextView
        android:id="@+id/highscoresLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:text="Highscores on this Device:"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="45dp" />


    <Button
        android:id="@+id/backBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Back"
        android:text="Back"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="392dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="41dp" />

    <TextView
        android:id="@+id/highscore1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/highscoresLabel"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:text="highscore1" />

    <TextView
        android:id="@+id/highscore2"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/highscore1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        android:text="highscore2" />

    <TextView
        android:id="@+id/highscore3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="highscore3"
        android:layout_below="@+id/highscore2"
        android:layout_alignStart="@+id/highscore2" />

    <TextView
        android:id="@+id/highscore4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/highscore3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:text="highscore4" />

    <TextView
        android:id="@+id/highscore5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/highscore4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:text="highscore5" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#1 - "
        android:layout_alignBaseline="@+id/highscore1"
        android:layout_alignBottom="@+id/highscore1"
        android:layout_toStartOf="@+id/highscore1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/highscore2"
        android:layout_toStartOf="@+id/highscore2"
        android:text="#2 - " />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/highscore3"
        android:layout_toStartOf="@+id/highscore3"
        android:text="#3 - " />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/highscore4"
        android:layout_alignBottom="@+id/highscore4"
        android:layout_toStartOf="@+id/highscore4"
        android:text="#4 - " />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/highscore5"
        android:layout_alignBottom="@+id/highscore5"
        android:layout_toStartOf="@+id/highscore5"
        android:text="#5 - " />

    <TextView
        android:id="@+id/score1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/highscore1"
        android:layout_toEndOf="@+id/highscore1"
        android:text="TextView" />

    <TextView
        android:id="@+id/score2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/highscore3"
        android:layout_toEndOf="@+id/highscore2"
        android:text="TextView" />

    <TextView
        android:id="@+id/score3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/score2"
        android:layout_alignTop="@+id/highscore3"
        android:text="TextView" />

    <TextView
        android:id="@+id/score4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/highscore5"
        android:layout_toEndOf="@+id/highscore4"
        android:text="TextView" />

    <TextView
        android:id="@+id/score5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/highscore5"
        android:layout_alignEnd="@+id/score4"
        android:layout_alignTop="@+id/highscore5"
        android:text="TextView" />


</RelativeLayout>
